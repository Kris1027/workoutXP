generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Exercise {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name        String
  category    String
  difficulty  String
  ImageUrl    String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
